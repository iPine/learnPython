    "from random import randint\n",
    "\n",
    "class Die():\n",
    "    \"\"\"表示一个骰子的类\"\"\"\n",
    "    \n",
    "    def __init__(self,num_sides=6):\n",
    "        \"\"\"骰子默认为6面\"\"\"\n",
    "        self.num_sides = num_sides\n",
    "        \n",
    "    def roll(self):\n",
    "        \"\"\"返回一个位于1和骰子面数之间的随机数\"\"\"\n",
    "        return randint(1,self.num_sides)"
   ]
