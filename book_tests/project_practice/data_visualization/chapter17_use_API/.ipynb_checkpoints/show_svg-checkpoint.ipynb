    "from flask import Flask  # 导入Flask包\n",
    "\n",
    "app = Flask(__name__)  # 获取Flask对象，以当前模块名为参数\n",
    "\n",
    "\n",
    "# 路由默认为（127.0.0.1:5000）\n",
    "@app.route('/')  # 装饰器对该方法进行路由设置，请求的地址\n",
    "def show_svg():  # 方法名称\n",
    "    return 'python_repos.svg'  # 返回响应的内容\n",
    "\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    app.run()\n"
